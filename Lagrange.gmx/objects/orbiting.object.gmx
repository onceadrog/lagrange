            <string>// Get g_offset parameters
room_scale = camera.room_scale;    

if (distance_to_object(moon)&lt;250){  //Assume moon SOI = 250px
    g_dist = distance_to_object(moon)*room_scale;
    SOI_grav = moon.grav;
    SOI_radius = moon.radius;
    SOI_rotation = moon.rotation;
    g_dir = point_direction(x, y, moon.x, moon.y);
} else {
    g_dist = distance_to_object(earth)*room_scale;
    SOI_grav = earth.grav;
    SOI_rotation = earth.rotation;
    SOI_radius = earth.radius;
    g_dir = point_direction(x, y,earth.x, earth.y); 
}

// Fall to surface
if (g_dist &gt; SOI_radius) {
    orbit = 1;
    if (speed &lt; 6){
        if (g_dist &gt; 500 &amp;&amp; g_dist &lt; 8000 &amp;&amp; speed &lt; 6 &amp;&amp; speed &gt; 5) {
            orbit = 2;    //LEO
            Angle = g_dir;
            }
        if (g_dist &gt; 34000 &amp;&amp; g_dist &lt; 38000 &amp;&amp; speed &lt; 3 &amp;&amp; speed &gt; 2) {
            orbit = 3; //GEO
            Angle = g_dir;
            }
    }
    } else {
    orbit_radius = SOI_radius/room_scale + sprite_width/2
    rotation = SOI_rotation;
    orbit = 0; } // Landed
switch (orbit) {    
    case 0: // Landed
        // Tidally locked rotation
        image_angle = point_direction(x, y, earth.x, earth.y);
        // Orbital motion
        Angle = g_dir + global.ns; //Apply global.ns to direction
        if(Angle &gt;= 360) Angle -= 360;
        if(Angle &lt; 0) Angle += 360;
        Orbit = 1000/room_scale;
        // Update position
        x = lengthdir_x(Orbit, Angle) + earth.x;
        y = lengthdir_y(Orbit, Angle) + earth.y;
        instance_destroy(); //Crash landed!
        break;
    case 1: // Wandering
        // Calculate g_offset
        g_offset = 1/((sqr(g_dist/SOI_radius))/SOI_grav);
        vxg = lengthdir_x(g_offset, g_dir);
        vyg = lengthdir_y(g_offset, g_dir);
        
        // Amend direction and speed
        // Apply g_offset,g_dir to speed,direction vector
        vx = lengthdir_x(speed, direction);
        vy = lengthdir_y(speed, direction);
        speed = sqrt(sqr(vx+vxg) + sqr(vy+vyg));
        direction = point_direction(0,0,vxg+vx,vyg+vy);
        
        // Update position and facing
        x = lengthdir_x(speed, direction) + x;
        y = lengthdir_y(speed, direction) + y;
        //image_angle = direction-90; Only for spaceships!
        break;
    case 2: // LEO
        // Orbital motion
        Angle += 6;
        if(Angle &gt;= 360) Angle -= 360;
        if(Angle &lt; 0) Angle += 360;
        Orbit = 36000/room_scale;
        // Update position
        x = lengthdir_x(Orbit, Angle) + earth.x;
        y = lengthdir_y(Orbit, Angle) + earth.y;
        break;
    case 3: // GEO
        // Orbital motion
        Angle += 1;
        if(Angle &gt;= 360) Angle -= 360;
        if(Angle &lt; 0) Angle += 360;
        Orbit = 1000/room_scale;
        // Update position
        x = lengthdir_x(Orbit, Angle) + earth.x;
        y = lengthdir_y(Orbit, Angle) + earth.y;
        break;
}

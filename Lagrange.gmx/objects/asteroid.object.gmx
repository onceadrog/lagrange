            <string>event_inherited();
// Create random asteroid
// type metallic, rocky, carbonaceous, icy
// content (percentage): rock, ice, carbon, metal (sum=1, can be mined lower)
randomize();
direction = random(360);
speed = random(4);
rotation_speed = random(6)-3;
type = irandom(3);
display_values = 1;
switch type{
    case 0:
        //Metallic
        type_name = "Metallic";
        metal = round(random(0.9)*100)/100;
        carbon = round(random(1-metal)*100)/100;
        rock = 1-metal-carbon;
        ice = 0;
        break;
    case 1:
        //Rocky
        type_name = "Rocky";
        rock = round(random(0.8)*100)/100;
        metal = round(random(1-rock)*100)/100;
        carbon = round(random(1-metal-rock)*100)/100;
        ice = 1-metal-carbon-rock;
        break;
    case 2:
        //Carbonaceous
        type_name = "Carbonaceous";
        carbon = round(random(0.6)*100)/100;
        rock = round(random(1-carbon)*100)/100;
        ice = 1-rock-carbon;
        metal = 0;
        break;
    case 3:
        //Icy
        type_name = "Icy";
        ice = round(random(0.9)*100)/100;
        carbon = round(random(1-ice)*100)/100; 
        rock = 1-ice-carbon;
        metal = 0;
        break;
}
edge_created = irandom(3);
switch edge_created {
    case 0:
        y=0;
        if (direction &lt; 180) direction += 180;
        break;
    case 1:
        x=room_width;
        if (direction &gt; 270 || direction &lt; 90){
            direction += 180;
            if (direction &gt; 360) direction -= 360;
        }
        break;
    case 2:
        y = room_height;
        if (direction &gt; 180) direction -= 180;
        break;
    case 3:
        x=0;
        if (direction &lt; 270 &amp;&amp; direction &gt; 90){
            direction += 180;
            if (direction &gt; 360) direction -= 360;
        }
        break;
}
</string>
